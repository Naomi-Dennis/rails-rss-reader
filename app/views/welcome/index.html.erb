<h1>Feeds</h1>

<% @articlesByDate.each_key do | feed | %>
    <h2><%= feed %></h2>

   <% @articlesByDate[feed].each do | date, articles | %> 
        <h3><%= date %></h3>
        <ul>
        <% articles.each do | article | %>
            <li>
               <h4><%= article.title %></h4>
               <%= article.description %> 
            </li>
        <% end %>
        </ul>
   <% end %> 
<% end %>