<h1>Add New Feed</h1>
<%= form_tag feeds_url, method:"post" do %>
    <%= text_field_tag "feed_url", nil,  size: "100px" %> <br/><br/>
    <%= submit_tag "Read" %> 
<% end %>
<hr />
<% unless @feeds.empty? %>
    <% @feeds.each do | feed | %>
         <%= link_to feed.name, feed_url(feed) %><br/>
    <% end %>
<% else %> 
    <p> Add a feed above</p>
<% end %> 