#view-feed-container {}

#view-feed-container h1 {
    text-align: center;
    font-size: 2.5em;
}

#view-feed-container .main-layout-left {
    float: left;
    width: 14%;
}

#view-feed-container .main-layout-right {
    float: right;
    width: 85%;
    height: 700px;
    overflow-y: scroll;
}

#view-feed-container .main-layout-right::after {
    float: clear;
}

#view-feed-container ul {
    list-style: none;
}

#view-feed-container a,
#view-feed-container a:hover {
    text-decoration: none;
    color: #000;
}

#view-feed-container h2,
#view-feed-container h1 {
    color: #fff;
    text-shadow: 1px 1px 5px #000;

}
#view-feed-container h1,
#view-feed-container h2,
#view-feed-container h3,
#view-feed-container p,
#view-feed-container span{
    -moz-user-select: none; 
    user-select: none; 
}

.main-layout-right ul li {
    border: 1px solid #000;
    background: rgba(255, 255, 255, 0.7);
    padding: 10px 30px;
    margin-bottom: 10px;
    box-shadow: 0px 5px 10px rgba(0, 0, 0, 0.5);
    cursor: pointer;
}

.main-layout-right ul li:hover {
    background: rgba(255, 255, 255, 1);
}

.main-layout-right h2 {
    text-align: center;
    margin-right: 20px;
    font-size: 2em
}

.main-layout-left h2 {
    text-align: center;
    margin-left: 20px;
}
.main-layout-left ul{
    overflow-y: auto;
}


.main-layout-left ul li {
    margin: 20px 0px;
    color: #000;
    font-weight: bold;
    padding: 5px 10px;
    cursor: pointer;
    border: 1px solid #000;
    background: rgba(255, 255, 255, 0.7);
}

.main-layout-left ul li:hover {
    background: rgba(255, 255, 255, 0.5);
}
#delete-feed-form{
text-align:right; 
}

#delete-feed-form input[type="submit"]{
    border-radius: unset;
    background: #ff8e8e;
    font-weight: bold; 
    user-select: none; 
    -moz-user-select: none;
    -webkit-user-select: none; 
}